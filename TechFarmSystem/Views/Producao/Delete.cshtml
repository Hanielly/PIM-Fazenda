@model TechFarmSystem.Models.Producao

@{
    ViewData["Title"] = "Excluir Produção";
}
<h2>Excluir Produção</h2>

<h3>Você tem certeza de que deseja deletar esse item&#63;</h3>
<div>
    <h4> Produção</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NomedoProduto)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NomedoProduto)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.QuantidadeProduzida)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.QuantidadeProduzida)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.DataProducao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DataProducao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Observacoes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Observacoes)
        </dd>
    </dl>

    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Excluir" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar para a Lista</a>
    </form>
</div>
